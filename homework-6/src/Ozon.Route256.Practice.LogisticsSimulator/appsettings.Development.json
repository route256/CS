{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kafka": {
    "Consumers": {
      "NewOrder": {
        "Topic": "new_orders",
        "Config": {
          "GroupId": "logistic_simulator_new_orders",
          "BootstrapServers": "localhost:29092",
          "EnableAutoCommit": false
        }
      }
    },
    "Producer": {
      "Config": {
        "BootstrapServers": "localhost:29092"
      },
      "OrderEventTopic": "order_events"
    }
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  }
}
